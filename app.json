{
  "name": "ᴇʀᴀʀᴏʙᴏᴛ",
  "description": "Telegram Group Management Bot Written In Python Using python-telegram-bot.",
  "repository": "https://github.com/IamDuru/ERAROBOT",
  "logo": "https://telegra.ph/file/61296490da95c55a1d5ee.jpg",
  "keywords": [
    "python3",
    "telegram",
    "bot",
    "ERAROBOT",
    "telegram-bot",
    "management",
    "ptb",
    "pyrogram"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    }
  ],
  "stack": "heroku-24",
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "14"
      }
    }
  ],
  "env": {
    "API_ID": {
      "description": "ɢᴇᴛ ᴛʜɪs ᴠᴀʟᴜᴇ ғʀᴏᴍ my.telegram.org/apps.",
      "required": true,
      "value":  "22652940"
    },
    "API_HASH": {
      "description": "ɢᴇᴛ ᴛʜɪs ᴠᴀʟᴜᴇ ғʀᴏᴍ my.telegram.org/apps.",
      "required": true,
      "value":  "91ec0cf5f722101c98fef8f0c6aedd0e"
    },
    "OWNER_ID": {
      "description": "ʏᴏᴜʀ ᴜsᴇʀ ID ᴀs ᴀɴ ɪɴᴛᴇɢᴇʀ.",
      "required": true,
      "value": "1679112664"
    },
    "DEV_USERS": {
      "description": "ᴅᴇᴠs ᴜsᴇʀ ID ᴀs ᴀɴ ɪɴᴛᴇɢᴇʀ.",
      "required": true,
      "value": "1679112664"
    },
    "OWNER_USERNAME": {
      "description": "ʏᴏᴜʀ ᴛɢ ᴜsᴇʀɴᴀᴍᴇ",
      "required": true,
      "value": "OfficialDurgesh"
    },
    "LOGGER_ID": {
      "description": "ᴇᴠᴇɴᴛ ʟᴏɢs ᴄʜᴀɴɴᴇʟ  ᴛᴏ ɴᴏᴛᴇ ᴅᴏᴡɴ ɪᴍᴘᴏʀᴛᴀɴᴛ ʙᴏᴛ ʟᴇᴠᴇʟ ᴇᴠᴇɴᴛs, ᴇx: '-10023456'",
      "required": true,
      "value":  "-1002243074516"
    },
    "MONGO_DB_URI": {
      "description": "ʀᴇǫᴜɪʀᴇᴅ ғᴏʀ ᴅᴀᴛᴀʙᴀsᴇ ᴄᴏɴɴᴇᴄᴛɪᴏɴs (ᴍᴏɴɢᴏ).",
      "required": true
    },
    "TOKEN": {
      "description": "Get bot token from @BotFather on TG",
      "required": true
    },
    "DEEP_API": {
      "description": "Your DEEP API key from https://deepai.org/",
      "required": false
    },
    "PYTHON_VERSION": {
      "description": "Python version to use",
      "value": "3.9.16"
    }
  },
  "scripts": {
    "postdeploy": "pip install -r requirements.txt"
  }
}
